<% if not @o_auths.blank? %>
  <hr />
  <% @o_auths.each do |o_auth| %>
    <%=
      react_component(
        'common/OAuthProviderLink',
        {
          oAuthId: o_auth.id,
          oAuthName: o_auth.name,
          oAuthLogo: o_auth.logo,
          oAuthReason: "login",
          isSignUp: defined?(is_sign_up) ? is_sign_up : false,
        }
      )
    %>
  <% end %>
<% end %>